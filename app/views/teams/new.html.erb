<% provide(:title, 'Add a Team') %>
<h1>Add a Team</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@team) do |f| %>

    

      <%= f.label :team_name %>
      <%= f.text_field :team_name %>

      <%= f.submit "Add Team", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
