<% provide(:title, 'All users') %>
<h1>All Members</h1>


<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      &nbsp; &nbsp; &nbsp; &nbsp; 
    | <%= link_to "Delete", user, method: :delete,
                                  data: { confirm: "Are you sure you want to delete this user?" }%>
    </li>
  <% end %>
</ul>
